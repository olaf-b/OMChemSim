within Simulator.Files;

package ThermodynamicPackages "Library of diferent Thermodynamic Property Packages used in Chemical Process Simulation"
  extends Modelica.Icons.VariantsPackage;

  annotation(
    Documentation(info = "<html><head></head><body>This is a package of different thermodynamic property packages that have been modeled in OpenModelica for Chemical Process Simulation. Currently, following thermodynamic packages are available:<div><br></div><div><ol><li>Raoults Law</li><li>NRTL</li><li>UNIQUAC<br></li><li>UNIFAC</li><li>Peng-Robinson</li><li>Grayson Streed</li></ol></div></body></html>"));
  
end ThermodynamicPackages;
